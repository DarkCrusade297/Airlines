@model Airlines.Models.SearchModel

<div id="results">
    <h3>Все рейсы: @Model.from.Name</h3>
    @foreach (var item in Model.flightsTo)
    {
        //Прямые рейсы
        <div class="container flight">
            <div class="row">
                <div class="col-lg-10 tickets">
                    <div class="col-lg-12 mainZone">
                        <div class="container col-lg-12">
                            <p class="airline">Авиалиния</p>
                            <div class="col-lg-4">
                                <p class="time">Время вылета</p>
                                <p class="info">Место вылета</p>
                                <p class="info">Дата вылета</p>
                            </div>
                            <div class="col-lg-4">
                                <div>
                                    <p class="tripTime">Время в пути:</p>
                                </div>
                                <div class="line">
                                </div>
                                <div class="col-lg-1 departurePlane">
                                    <svg class="svgD" width="24" height="24" viewBox="0 0 24 24" fill="#A0B0B9" xmlns="http://www.w3.org/2000/svg">
                                        <path fill-rule="evenodd" clip-rule="evenodd" d="M17 13H14C12.4577 11.5739 10.1159 9.82474 8.16374 8.01001L6.84295 8C7.60515 9.49106 8.69945 11.4376 9.5 13H6.5C5.54148 12.2794 4.45139 11.6727 3.69008 11.1028C3.27567 10.7932 2.80644 11.0649 3.08297 11.6043C4.38443 14.1509 4.90267 15.176 4.90267 15.176C5.12943 15.6181 5.48619 15.9964 6 16H19.7317C20.7473 15.9659 20.9885 15.3663 21 14.7944C21 13.5 19.5675 13.0196 17 13Z">
                                        </path>
                                    </svg>
                                    <p class="airport">JFK</p>
                                </div>
                                <div class="col-lg-9"></div>
                                <div class="col-lg-1 arrivalPlane">
                                    <svg class="svgA" width="24" height="24" viewBox="0 0 24 24" fill="#A0B0B9" xmlns="http://www.w3.org/2000/svg">
                                        <path fill-rule="evenodd" clip-rule="evenodd" d="M17 13H14C12.4577 11.5739 10.1159 9.82474 8.16374 8.01001L6.84295 8C7.60515 9.49106 8.69945 11.4376 9.5 13H6.5C5.54148 12.2794 4.45139 11.6727 3.69008 11.1028C3.27567 10.7932 2.80644 11.0649 3.08297 11.6043C4.38443 14.1509 4.90267 15.176 4.90267 15.176C5.12943 15.6181 5.48619 15.9964 6 16H19.7317C20.7473 15.9659 20.9885 15.3663 21 14.7944C21 13.5 19.5675 13.0196 17 13Z">
                                        </path>
                                    </svg>
                                    <p class="airport">KHV</p>
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <p class="time">Время прилета</p>
                                <p class="info">Место прилёта</p>
                                <p class="info">Дата прилёта</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-2 btnZone">
                    <button class="btn btnOrder"><strong>Забронировать</strong></button>
                </div>
            </div>
        </div>
        <div class="col-lg-12 empty"></div>
    }
</div>
